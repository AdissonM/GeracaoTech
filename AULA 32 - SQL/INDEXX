
CREATE DATABASE digital;

CREATE TABLE students (
id INT PRIMARY KEY AUTO_INCREMENT,
name VARCHAR(150) NOT NULL,
email VARCHAR(50) NOT NULL,
date_register DATE NOT NULL
);

INSERT INTO students (name, email, date_register)
